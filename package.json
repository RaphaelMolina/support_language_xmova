{
  "name": "xmova-language",
  "displayName": "Xmova Language",
  "description": "Support for Language Xmova",
  "icon": "img/Logo.png",
  "version": "0.0.19",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaphaelMolina/support_language_xmova"
  },
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "publisher": "RaphaelMolina",
  "contributes": {
    "languages": [{
      "id": "xmova",
      "aliases": ["Xmova", "xmova"],
      "extensions": [".xmv"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "icons/icon.svg",
        "dark": "icons/icon.svg"
      }
    }],
    "grammars": [{
      "language": "xmova",
      "scopeName": "source.xmv",
      "path": "./syntaxes/xmova.tmLanguage.json"
    }],
    "snippets": [{
      "language": "xmova",
      "path": "./snippets/snippets.json"
    }]
  }
}
